<div class="jumbotron">
  <h1>Vinyard Salon</h1>

  #if($stylists)
    <p>Click on a stylist to see their clients:</p>
    <ul>
      #foreach ($stylist in $stylists)
        <li><a href="/stylists/$stylist.getId()">$stylist.getStylistName()</a></li>
      #end
    </ul>
  #else
    <p>There are no stylists yet!</p>
  #end

<a class="btn btn-info" href="/stylists/new">Add a new Stylist</a>
