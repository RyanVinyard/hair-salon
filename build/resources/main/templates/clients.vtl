<h1>Client List</h1>

#foreach ($client in $clients)
  <h4><a href="/client/$client.getId()">$client.getClientName()</a></h4>
  <p>$client.getInfo()</p>
    <form action="/clients/$client.getId()/delete" method="post">
      <button type="submit" class="btn" id="deleteClient">Delete</button>
    </form>
#end

<a href="/stylists/$stylist.getId()/clients/new">Add a new client</a>
